class Solution {
    public String decodeString(String s) {
        StringBuilder builder = new StringBuilder();
        // å°†å­—ç¬¦ä¸²åè½¬
        String s1 = builder.append(s).reverse().toString();
        // å°†builderå½“ä½œæ ˆ
        builder = new StringBuilder();
        String substring = null;
        for (int i = 0; i < s1.length() ; i++) {
                // å¦‚æœé‡åˆ°çš„æ˜¯å­—ç¬¦æˆ–è€…']'ï¼Œå‹å…¥æ ˆ
                if(s1.charAt(i)>='a' || s1.charAt(i)==']')
                    builder.append(s1.charAt(i));
                // å¦‚æœé‡åˆ° '['
                else if (s1.charAt(i)=='['){
                  // æœç´¢ç¦»æ ˆé¡¶æœ€è¿‘çš„']'
                    int j = builder.lastIndexOf("]");
                //è®°å½•å½“å‰æ ˆçš„é«˜åº¦
                    int k  = builder.toString().length();
                    // æ±‚å¾—'[]' ä¹‹é—´çš„å­—ç¬¦ä¸²
                    substring = builder.toString().substring(j+1, k);
                    // å°†']'åŠä¹‹åçš„å…ƒç´ å¼¹å‡ºæ ˆ
                    builder.delete(j,k);
                } else {
                    // é‡åˆ°æ•°å­—
                     StringBuilder builder1 = new StringBuilder();
                    // æ•°å­—å¯èƒ½æœ‰å¤šä½
                    while (i<s1.length()&& s1.charAt(i)>='0'&&s1.charAt(i)<='9'){
                        builder1.append(s1.charAt(i));
                        i++;
                    }
                    // è¿™é‡Œè¦i--,å› ä¸ºforå¾ªç¯è¿˜è¦i++
                    i--;
                    int n  = Integer.parseInt(builder1.reverse().toString());
                    //  å°†nä¸ª'[]'ä¹‹é—´çš„å­—ç¬¦ä¸²å‹å…¥æ ˆ
                    while (n-- >0)
                        builder.append(substring);
                }               
        }
        // å› ä¸ºæ˜¯å€’åºéå†ï¼Œæ‰€ä»¥è¦åè½¬ç»“æœ
        return builder.reverse().toString();
    }
}
